{
    "expo": {
        "name": "Blog App",
        "slug": "blog-app",
        "privacy": "public",
        "sdkVersion": "35.0.0",
        "platforms": [
            "ios",
            "android"
        ],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.tarasburdeniuk.blog-app-rn",
            "buildNumber": "1.0.0"
        },
        "android": {
            "package": "com.tarasburdeniuk.blog_app_rn",
            "versionCode": 1,
            "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
        }
    }
}
